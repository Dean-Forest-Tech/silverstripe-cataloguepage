{
    "name": "dft/silverstripe-cataloguepage",
    "description": "Silverstripe module that creates a simple page in the CMS to map products and categories to.",
    "type": "silverstripe-vendormodule",
    "homepage": "http://github.com/Dean-Forest-Tech/silverstripe-cataloguepage",
    "keywords": ["silverstripe", "commerce", "ecommerce", "product", "category", "catalogue", "page"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Dean Forest Tech",
            "homepage": "https://dean-forest-tech.uk"
        }
    ],
    "support": {
        "issues": "http://github.com/Dean-Forest-Tech/silverstripe-catalogue/issues"
    },
    "require": {
        "silverstripe/cms": "^4 || ^5"
    },
    "suggest": {
        "silvercommerce/catalogue-admin": "*"
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "DFT\\SilverStripe\\CataloguePage\\": "src/"
        }
    }
}
